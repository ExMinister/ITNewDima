<Window x:Class="IT_Department.View.OrgTexnical"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IT_Department.View"
        mc:Ignorable="d"
        Title="OrgTexnical" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <DataGrid Grid.Row="0" />
        <DockPanel Grid.Row="1">
            <Button Content="Сохранить/Изменить" HorizontalAlignment="Left" Margin="15,5" />
            <Button Content="Удалить" HorizontalAlignment="Right" Margin="15,5" />
        </DockPanel>
    </Grid>
</Window>
